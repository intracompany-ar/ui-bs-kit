{
  "name": "@intracompany/ui-bs-kit",
  "version": "1.0.21",
  "description": "Componentes Bootstrap 5 UI",
  "keywords": [
    "frontend"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./ui-bs-kit.css": {
      "default": "./dist/ui-bs-kit.css"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ui-bs-kit"
  },
  "license": "MIT",
  "author": "IntraCompany",
  "type": "module",
  "scripts": {
    "local": "npx vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run type-check && vite build",
    "type-check": "vue-tsc --noEmit",
    "copy:audio": "cp -R src/audio dist",
    "copy:fonts": "cp -R src/fonts dist",
    "release": "npm run copy:audio && npm run copy:fonts && npm run build && npm run types && npm version patch && npm publish --access public && git push",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.32",
    "@vitest/ui": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^26.0.0",
    "sass": "1.77",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "peerDependencies": {
    "@intracompany/commons_front": "^1.2",
    "@selectize/selectize": "^0.15",
    "@vitejs/plugin-vue": "^5.0",
    "axios": "^1.6",
    "bootstrap": "^5.0.0",
    "datatables.net-bs5": "^2.2",
    "datatables.net-buttons-bs5": "^3.2",
    "dayjs": "^1.11",
    "vite": "^6.0",
    "vue": "^3.0",
    "vue-router": "^4.1"
  }
}
