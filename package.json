{
  "name": "@intracompany/ui-bs-kit",
  "version": "1.0.2",
  "description": "Componentes Bootstrap 5 UI",
  "keywords": [
    "frontend"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./ui-bs-kit.css": {
      "default": "./dist/ui-bs-kit.css"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ui-bs-kit"
  },
  "license": "MIT",
  "author": "IntraCompany",
  "type": "module",
  "scripts": {
    "copy:vue": "cp -R src/components dist && cp -R src/layouts dist",
    "copy:audio": "cp -R src/audio dist",
    "copy:css": "cp -R src/css dist",
    "copy:fonts": "cp -R src/fonts dist",
    "build": "vite build",
    "release": "npm run copy:vue && npm run copy:audio && npm run copy:css && npm run copy:fonts && vite build && npm version patch && npm publish --access public && git push"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "sass": "1.77"
  },
  "peerDependencies": {
    "@intracompany/commons_front": "file:../commons_front",
    "@selectize/selectize": "^0.15",
    "@vitejs/plugin-vue": "^5.0",
    "axios": "^1.6",
    "bootstrap": "^5.0.0",
    "datatables.net-bs5": "^2.2",
    "dayjs": "^1.11",
    "vite": "^6.0",
    "vue": "^3.0",
    "vue-router": "^4.1",
    "datatables.net-buttons-bs5": "^3.2"
  }
}
